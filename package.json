{
  "name": "interpreter",
  "version": "1.0.0",
  "description": "A minimal javascript interpreter",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack",
    "build": "webpack --config webpack.prod.js",
    "lint": "npx tslint -c tslint.json 'src/*.ts'",
    "travis": "npm run build && npm run test && tslint -c tslint.json 'src/*.ts'",
    "generator": "node tools/generator.js",
    "cli": "node dist/@script",
    "test": "jasmine"
  },
  "keywords": [
    "javascript",
    "interpreter"
  ],
  "author": "eugenioenko",
  "license": "MIT",
  "devDependencies": {
    "jasmine": "^5.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@types/node": "^20.3.2"
  }
}
