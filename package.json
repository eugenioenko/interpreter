{
  "name": "interpreter",
  "version": "1.0.0",
  "description": "A minimal javascript interpreter",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack",
    "build": "webpack --config webpack.prod.js",
    "lint": "npx tslint -c tslint.json 'src/*.ts'",
    "travis": "npm run build && npm run test && tslint -c tslint.json 'src/*.ts'",
    "generator": "node tools/generator.js",
    "cli": "node dist/@script",
    "test": "jasmine"
  },
  "keywords": [
    "javascript",
    "interpreter"
  ],
  "author": "eugenioenko",
  "license": "MIT",
  "devDependencies": {
    "jasmine": "^3.3.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^9.4.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3333",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@types/node": "^12.12.3"
  }
}
